a=int(input("Enter the 1st number: "))
b=int(input("Enter the 2nd number: "))
n=int(input("Enter the no.of elements to display: "))
if n<=0:
    print("Please enter positive values")
elif n==1:
    print(a)
else:
    print(a)
    print(b)
    for i in range(2,n):
        c=a+b
        a=b
        b=c
        print(c)
